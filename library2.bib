	Mesh = {Arabidopsis; Arabidopsis Proteins; Base Sequence; DNA Mutational Analysis; Gene Expression Regulation, Plant; Genome, Plant; MicroRNAs; Molecular Sequence Data; Mutation; Nucleic Acid Conformation; RNA Precursors; Sequence Analysis, DNA},
	Month = {Jan},
	Number = {1},
	Pages = {466-71},
	Pmc = {PMC2806713},
