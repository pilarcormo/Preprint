	Keywords = {Arabidopsis; EMS; NextGen-Sequencing; mutant-mapping; mutant-screen; parental-backcross},
	Pages = {362},
	Pmc = {PMC3772335},
	Pmid = {24062760},
	Pst = {epublish},
	Title = {Facile mutant identification via a single parental backcross method and application of whole genome sequencing based mapping pipelines},
	Volume = {4},
	Year = {2013},
