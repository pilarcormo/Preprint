	Mesh = {Arabidopsis; Arabidopsis Proteins; Brassica rapa; Chromosome Mapping; DNA Mutational Analysis; DNA, Plant; Flowers; Gene Frequency; Gene Library; Genetic Linkage; Genome, Plant; High-Throughput Nucleotide Sequencing; MADS Domain Proteins; Mutation; Sequence Analysis, DNA; Synteny; Transcriptome},
	Month = {Aug},
